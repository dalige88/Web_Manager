@using Web.Manager.WebManager.Entity;

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "日志";
}
@await Html.PartialAsync("_nav", new WebManager.Core.Entity.NavigationModel("系统管理", "日志"))
    <style type="text/css">

        .autocut {
            width: 300px;
            overflow: hidden;
            white-space: nowrap;
            text-overflow: ellipsis;
            -o-text-overflow: ellipsis;
            -icab-text-overflow: ellipsis;
            -khtml-text-overflow: ellipsis;
            -moz-text-overflow: ellipsis;
            -webkit-text-overflow: ellipsis;
        }
    </style>
<div class="row bgwhitebox10 mymgb15">
    <div class="detail-search" style="border-bottom: 2px solid #01ACE2;padding-bottom:10px;">
        <div class="mycl"></div>
    </div>
    <div class="row mtop15">
        <div class="row">
            <div class="col-sm-12" style="padding:0px;">
                <table class="table table-bordered mytable">
                    <thead>
                        <tr>
                            <th width="5%">ID</th>
                            <th width="15%">operation</th>
                            <th width="15%">URL</th>
                            <th width="20%">Param</th>
                            <th width="10%">Uid</th>
                            <th width="10%">Account</th>
                            <th width="25%">CreateTime</th>
                            <th width="10%">IP</th>
                        </tr>
                    </thead>
                    <tbody id="accountbody"></tbody>
                </table>
            </div>
            <div class="row">
                <div id="infoPage"></div>
            </div>
        </div>
    </div>
</div>
@section CssExtend{
    <link rel="stylesheet" href="@HtmlExtention.GetUrlContent("/Scripts/plugins/sspager/ss_page.css")" />
}
@section JsExtend{
    <script type="text/javascript" src="@HtmlExtention.GetUrlContent("/Scripts/plugins/sspager/ss_page.js")"></script>
    <script type="text/javascript" src="@HtmlExtention.GetUrlContent("/Scripts/WebManager/system/logslist.js")"></script>
}



